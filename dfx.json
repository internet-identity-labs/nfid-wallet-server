{
  "canisters": {
    "identity_manager": {
      "build": "src/identity_manager/build.sh",
      "candid": "src/identity_manager/identity_manager.did",
      "wasm": "identity_manager.wasm",
      "type": "custom"
    },
    "identity_manager_replica": {
      "build": "src/identity_manager/build.sh",
      "candid": "src/identity_manager/identity_manager.did",
      "wasm": "identity_manager.wasm",
      "type": "custom"
    },
    "identity_manager_test_replica": {
      "build": "src/identity_manager/build.sh",
      "candid": "src/identity_manager/identity_manager.did",
      "wasm": "identity_manager.wasm",
      "type": "custom"
    },
    "pub_sub_channel": {
      "build": "src/pub_sub_channel/build.sh",
      "candid": "src/pub_sub_channel/pub_sub_channel.did",
      "wasm": "pub_sub_channel.wasm",
      "type": "custom"
    },
    "im_addition": {
      "build": "src/im_addition/build.sh",
      "candid": "src/im_addition/im_addition.did",
      "wasm": "im_addition.wasm",
      "type": "custom"
    },
    "verifier": {
      "build": "src/verifier/build.sh",
      "candid": "src/verifier/verifier.did",
      "wasm": "verifier.wasm",
      "type": "custom"
    },
    "internet_identity_test": {
      "type": "custom",
      "candid": "src/internet_identity_test/internet_identity.did",
      "wasm": "internet_identity.wasm",
      "build": "curl -sSL https://github.com/dfinity/internet-identity/releases/latest/download/internet_identity_test.wasm -o internet_identity.wasm"
    }
  },
  "networks": {
    "dev": {
      "providers": ["https://ic0.app/"],
      "type": "persistent"
    },
    "prod": {
      "providers": ["https://ic0.app/"],
      "type": "persistent"
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  },
  "version": 1
}
